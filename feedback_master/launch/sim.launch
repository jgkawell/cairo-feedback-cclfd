<launch>
    <!-- Setup parameters -->
    <param name="QUERY_STRATEGY" type="string" value="none" />

    <!-- Launch classifiers -->
    <node name="synthesizer" pkg="feedback_classifiers" type="synthesizer.py" output="screen" />

    <!-- Launch planning nodes -->
    <node name="query_nlp" pkg="feedback_planners" type="query_nlp.py" output="screen" />
    <node name="demonstrator" pkg="feedback_planners" type="demonstrator.py" output="screen" />
    <node name="request_feedback_server" pkg="feedback_planners" type="request_feedback_server.py" output="screen" launch-prefix="xterm -e" />
    <node name="teleop" pkg="feedback_master" type="teleop.py" output="screen" launch-prefix="xterm -e" />

    <!-- CC-LfD PARAMETERS -->
    <param name="CONFIG" type="string" value="$(find lfd_experiments)/experimental_data/feedback_demo/config.json"/>
    <param name="DEMO_DIRECTORY" type="string" value="$(find lfd_experiments)/experimental_data/feedback_demo/new_data/labeled"/>
    <node name="feedback_demonstration" pkg="lfd_experiments" type="feedback_demonstration_server.py" output="screen" />
</launch>